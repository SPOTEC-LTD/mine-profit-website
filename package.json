{
  "name": "nuxt-web-desgin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env API=dev nuxt dev",
    "buildTest": "cross-env API=dev nuxt build",
    "onlineTest": "cross-env API=onlineTest nuxt build",
    "build": "cross-env API=pro nuxt build",
    "static": "cross-env API=static nuxt dev -p 3002",
    "start": "nuxt start -p 4567",
    "testStart": "nuxt start -p 4567",
    "mockServer": "nodemon ./mock/server/index.js --exec babel-node -e js",
    "i18n:merge": "babel-node build/i18n/mergeEnTranslate.js",
    "i18n:export": "babel-node build/i18n/mergeTranslate.js exportNoTranslate",
    "build:i18n": "cross-env NODE_ENV=production babel-node build/i18n/index.js",
    "i18n:watch": "nodemon --watch ./src/shared/intl/messages build/i18n/index.js --exec babel-node -e js",
    "eslint": "eslint --cache --quiet --ext .js,.jsx,.vue ./",
    "eslint:fix": "eslint --fix --quiet --cache --ext .js,.jsx,.vue --format=pretty ./",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "test": "jest"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
